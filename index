<script>
// â•â• ê³µí†µ ì „ì†¡ í•¨ìˆ˜ (Google Apps Script) â•â•
var GAS_URL = 'https://script.google.com/macros/s/AKfycbzwBmB9B7FEte58bA4t_YfSJIZTwgIfSemHhyfvBj-bU6cqc2slxJyYPq_GA3t1o0Ed/exec';

function sendToGAS(payload) {
  if (!payload.name  && payload['ì´ë¦„'])   payload.name  = payload['ì´ë¦„'];
  if (!payload.phone && payload['ì—°ë½ì²˜']) payload.phone = payload['ì—°ë½ì²˜'];
  if (!payload.time)  payload.time  = new Date().toLocaleString('ko-KR', {timeZone:'Asia/Seoul'});
  if (!payload.type)  payload.type  = payload['ìœ í˜•'] || 'ìƒë‹´ì‹ ì²­';

  // no-cors ì œê±° + Content-Type: text/plain â†’ GASê°€ postData.contents ì •ìƒ ìˆ˜ì‹ 
  fetch(GAS_URL, {
    method: 'POST',
    headers: {'Content-Type': 'text/plain'},
    body: JSON.stringify(payload)
  }).catch(function(){});
}
</script>
