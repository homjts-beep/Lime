<!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ì§„ë‹¨ ëª¨ë‹¬ JS â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
<script>
// ê³µí†µ
function mOpen(id){
  var el=document.getElementById(id);
  if(id==='ins-modal'){el.style.display='block';document.body.style.overflow='hidden';}
  else{el.classList.add('on');document.body.style.overflow='hidden';}
}
function mClose(id){
  var el=document.getElementById(id);
  if(id==='ins-modal'){el.style.display='none';document.body.style.overflow='';}
  else{el.classList.remove('on');document.body.style.overflow='';}
}
function mPhone(el){let v=el.value.replace(/\D/g,'');if(v.length<=3)el.value=v;else if(v.length<=7)el.value=v.slice(0,3)+'-'+v.slice(3);else el.value=v.slice(0,3)+'-'+v.slice(3,7)+'-'+v.slice(7,11)}

// ëª¨ë‹¬1 - êµí†µì‚¬ê³ 
function m1pick(el,i){el.closest('.mopts').querySelectorAll('.mopt').forEach(o=>o.classList.remove('on'));el.classList.add('on');document.getElementById('m1n'+i).disabled=false}
function m1multi(el,i){el.classList.toggle('on');document.getElementById('m1n'+i).disabled=el.closest('.mopts').querySelectorAll('.mopt.on').length===0}
function m1step0next(){
  // 12ëŒ€ ì¤‘ê³¼ì‹¤ ì„ íƒí–ˆìœ¼ë©´ ov2ë¡œ ì „í™˜
  const sel=document.querySelector('#m1s0 .mopt.on');
  if(sel&&sel.querySelector('.mic').textContent==='ğŸš¨'){mClose('ov1');setTimeout(()=>mOpen('ov2'),200);return}
  m1go(1)
}
function m1go(to){
  document.getElementById('m1s'+(to-1)).classList.remove('on');
  document.getElementById('m1s'+to).classList.add('on');
  document.getElementById('m1p'+(to-1)).className='mpd mdn';
  document.getElementById('m1p'+to).className='mpd mact';
  document.getElementById('mb1').scrollTop=0
}
function m1result(){
  document.getElementById('m1s4').classList.remove('on');
  document.getElementById('m1p4').className='mpd mdn';
  document.getElementById('mp1').style.display='none';
  document.getElementById('m1res').classList.add('on');
  document.getElementById('mb1').scrollTop=0
}
function m1sub(){
  var name=document.getElementById('m1name').value.trim();
  var phone=document.getElementById('m1phone').value.trim();
  if(!name){alert('ì´ë¦„ì„ ì…ë ¥í•´ ì£¼ì„¸ìš”');return}
  if(phone.length<12){alert('ì—°ë½ì²˜ë¥¼ ì •í™•íˆ ì…ë ¥í•´ ì£¼ì„¸ìš”');return}
  var step1=(document.querySelector('#m1s0 .mopt.on .mtx')||{textContent:'-'}).textContent.trim();
  var step2=(document.querySelector('#m1s1 .mopt.on .mtx')||{textContent:'-'}).textContent.trim();
  var step3=(document.querySelector('#m1s2 .mopt.on .mtx')||{textContent:'-'}).textContent.trim();
  var step4=Array.from(document.querySelectorAll('#m1s3 .mopt.on .mtx')).map(function(e){return e.textContent.trim();}).join(', ')||'-';
  var step5=(document.querySelector('#m1s4 .mopt.on .mtx')||{textContent:'-'}).textContent.trim();
  sendToGAS({
    name:name, phone:phone,
    step1:step1, step2:step2, step3:step3, step4:step4, step5:step5,
    type:'30ì´ˆ êµí†µì‚¬ê³  ë¬´ë£Œ ì§„ë‹¨',
    time:new Date().toLocaleString('ko-KR',{timeZone:'Asia/Seoul'})
  });
  document.getElementById('m1res').classList.remove('on');
  document.getElementById('m1done').classList.add('on');
  document.getElementById('mb1').scrollTop=0;
}

// ëª¨ë‹¬2 - 12ëŒ€ ì¤‘ê³¼ì‹¤
function m2pick(el,i){el.closest('.mopts').querySelectorAll('.mopt').forEach(o=>o.classList.remove('on'));el.classList.add('on');document.getElementById('m2n'+i).disabled=false}
function m2multi(el,i){el.classList.toggle('on');document.getElementById('m2n'+i).disabled=el.closest('.mopts').querySelectorAll('.mopt.on').length===0}
function m2go(to){
  document.getElementById('m2s'+(to-1)).classList.remove('on');
  document.getElementById('m2s'+to).classList.add('on');
  document.getElementById('m2p'+(to-1)).className='mpd mdn r';
  if(to<5)document.getElementById('m2p'+to).className='mpd mact r';
  document.getElementById('mb2').scrollTop=0
}
function m2sub(){
  var name=document.getElementById('m2name').value.trim();
  var phone=document.getElementById('m2phone').value.trim();
  if(!name){alert('ì´ë¦„ì„ ì…ë ¥í•´ ì£¼ì„¸ìš”');return}
  if(phone.length<12){alert('ì—°ë½ì²˜ë¥¼ ì •í™•íˆ ì…ë ¥í•´ ì£¼ì„¸ìš”');return}
  var step1=(document.querySelector('#m2s0 .mopt.on .mtx')||{textContent:'-'}).textContent.trim();
  var step2=(document.querySelector('#m2s1 .mopt.on .mtx')||{textContent:'-'}).textContent.trim();
  var step3=(document.querySelector('#m2s2 .mopt.on .mtx')||{textContent:'-'}).textContent.trim();
  var step4=Array.from(document.querySelectorAll('#m2s3 .mopt.on .mtx')).map(function(e){return e.textContent.trim();}).join(', ')||'-';
  sendToGAS({
    name:name, phone:phone,
    step1:step1, step2:step2, step3:step3, step4:step4, step5:'-',
    type:'12ëŒ€ ì¤‘ê³¼ì‹¤ ì‚¬ê³  ë¬´ë£Œ ì§„ë‹¨',
    time:new Date().toLocaleString('ko-KR',{timeZone:'Asia/Seoul'})
  });
  document.getElementById('m2s4').classList.remove('on');
  document.getElementById('mp2').style.display='none';
  document.getElementById('m2done').classList.add('on');
  document.getElementById('mb2').scrollTop=0;
}
</script>

<script>
// Tab
function showTab(id, btn) {
  document.querySelectorAll('.accident-content').forEach(c => c.classList.remove('active'));
  document.querySelectorAll('.tab-btn').forEach(b => b.classList.remove('active'));
  document.getElementById('tab-' + id).classList.add('active');
  btn.classList.add('active');
}

// FAQ
function toggleFaq(el) {
  const isOpen = el.classList.contains('open');
  document.querySelectorAll('.faq-item').forEach(i => {
    i.classList.remove('open');
    i.querySelector('.faq-a').style.maxHeight = '0';
  });
  if (!isOpen) {
    el.classList.add('open');
    el.querySelector('.faq-a').style.maxHeight = '300px';
  }
}

// Scroll reveal
const obs = new IntersectionObserver(entries => {
  entries.forEach(e => { if (e.isIntersecting) e.target.classList.add('on'); });
}, { threshold: 0.08 });
document.querySelectorAll('.reveal').forEach(el => obs.observe(el));
</script>


