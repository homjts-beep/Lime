<script>
// â”€â”€ ë³´í—˜ ëª¨ë‹¬ ë¡œì§ â”€â”€
var insState = { purpose:'', age:'', covers:[], premium:'', name:'', phone:'' };
var insCoverLabels = { cancer:'ì•” ì§„ë‹¨ë¹„', heart:'ë‡ŒÂ·ì‹¬ì¥', real:'ì‹¤ì†ë³´í—˜', injury:'ìƒí•´Â·ê³¨ì ˆ', pension:'ì—°ê¸ˆÂ·ì €ì¶•', life:'ì¢…ì‹ Â·ì‚¬ë§', dement:'ì¹˜ë§¤Â·ê°„ë³‘', child:'ì–´ë¦°ì´ë³´í—˜' };

function insGo(id) {
  document.querySelectorAll('.ins-slide').forEach(function(s){s.classList.remove('active');});
  document.getElementById(id).classList.add('active');
  var steps = {ins_s1:1,'ins-s1':1,'ins-s2':2,'ins-s3':3,'ins-s4':4,'ins-s5':5,'ins-result':5};
  document.getElementById('ins-prog').style.width = ((steps[id]||1)/5*100)+'%';
  document.getElementById('ins-wrap').scrollTop = 0;
}
function insSelect(el, key, val) {
  insState[key] = val;
  var parent = el.parentElement;
  parent.querySelectorAll('.ins-choice, .ins-choice-sm').forEach(function(b){b.classList.remove('sel');});
  el.classList.add('sel');
}
function insToggleTag(el, key) {
  el.classList.toggle('sel');
  var idx = insState.covers.indexOf(key);
  if(idx>-1) insState.covers.splice(idx,1); else insState.covers.push(key);
}
function insFormatPhone(input) {
  var v=input.value.replace(/[^0-9]/g,'');
  if(v.length>11)v=v.slice(0,11);
  if(v.length>=8)v=v.slice(0,3)+'-'+v.slice(3,7)+'-'+v.slice(7);
  else if(v.length>=4)v=v.slice(0,3)+'-'+v.slice(3);
  input.value=v;
}
function insSubmit() {
  var name = document.getElementById('ins-name').value.trim();
  var phone = document.getElementById('ins-phone').value.trim();
  if(!name){alert('ì´ë¦„ì„ ì…ë ¥í•´ ì£¼ì„¸ìš”');return;}
  if(phone.length<12){alert('ì—°ë½ì²˜ë¥¼ ì˜¬ë°”ë¥´ê²Œ ì…ë ¥í•´ ì£¼ì„¸ìš”');return;}
  insState.name = name; insState.phone = phone;

  var purposeMap = {new:'ì‹ ê·œ ë³´í—˜ ê°€ì…',remodel:'ë³´í—˜ ë¦¬ëª¨ë¸ë§',both:'ê°€ì…+ë¦¬ëª¨ë¸ë§ ëª¨ë‘'};
  var ageMap = {'20s':'20ëŒ€','30s':'30ëŒ€','40s':'40ëŒ€','50s':'50ëŒ€','60s':'60ëŒ€','70+':'70ëŒ€ ì´ìƒ'};
  var premiumMap = {under5:'5ë§Œì› ë¯¸ë§Œ','5to15':'5~15ë§Œì›','15to30':'15~30ë§Œì›',over30:'30ë§Œì› ì´ìƒ',none:'ì—†ìŒ/ëª¨ë¦„'};
  var coverText = insState.covers.map(function(c){return insCoverLabels[c]||c;}).join(', ') || 'ë¯¸ì„ íƒ';

  var summaryLines = [
    'ğŸ“Œ ìƒë‹´ ëª©ì : ' + (purposeMap[insState.purpose]||'-'),
    'ğŸ‘¤ ì—°ë ¹ëŒ€: ' + (ageMap[insState.age]||'-'),
    'ğŸ›¡ ê´€ì‹¬ ë³´ì¥: ' + coverText,
    'ğŸ’° í˜„ì¬ ì›” ë³´í—˜ë£Œ: ' + (premiumMap[insState.premium]||'-'),
    'ğŸ“ ì—°ë½ì²˜: ' + phone.slice(0,7)+'-****'
  ];
  document.getElementById('ins-result-summary').innerHTML = summaryLines.join('<br>');

  // GAS ì›¹í›… ì „ì†¡ (GASì—ì„œ homjts@gmail.comìœ¼ë¡œ ì´ë©”ì¼ ë°œì†¡)
  sendToGAS({
    name: name, phone: phone,
    step1: purposeMap[insState.purpose]||'-',
    step2: ageMap[insState.age]||'-',
    step3: coverText,
    step4: premiumMap[insState.premium]||'-',
    step5: '-',
    type: 'ë³´í—˜ê°€ì…Â·ë¦¬ëª¨ë¸ë§ ìƒë‹´ì‹ ì²­',
    time: new Date().toLocaleString('ko-KR',{timeZone:'Asia/Seoul'})
  });

  insGo('ins-result');
}

function insReset() {
  insState = { purpose:'', age:'', covers:[], premium:'', name:'', phone:'' };
  document.getElementById('ins-name').value='';
  document.getElementById('ins-phone').value='';
  document.querySelectorAll('.ins-choice,.ins-choice-sm,.ins-tag-btn').forEach(function(b){b.classList.remove('sel');});
  insGo('ins-s1');
}
</script>

<script>
// â”€â”€ ì§„ë‹¨ í¼ ë¡œì§ (ëª¨ë‹¬3) â”€â”€
const dState = { bodyPart:null, options:{}, score:0, name:'', phone:'' };

function dUpdateProgress(step) {
  document.getElementById('dProgressBar').style.width = (step/6*100)+'%';
}
function dGoTo(slideId) {
  document.querySelectorAll('.d-slide').forEach(function(s){s.classList.remove('active');});
  document.getElementById(slideId).classList.add('active');
  var stepMap={dslide1:1,dslide2a:2,dslide2b:2,dslide2c:2,dslide3a:3,dslide3b:3,dslide3c:3,dslide4:4,dslide5:5,dslide6:6};
  dUpdateProgress(stepMap[slideId]||1);
  document.getElementById('diag-wrap').scrollTop=0;
}
function dSelectBodyPart(part) {
  dState.bodyPart=part; dState.score=0;
  if(part==='spine') dGoTo('dslide2a');
  else if(part==='pin'){dState.score+=3;dGoTo('dslide2b');}
  else dGoTo('dslide2c');
}
function dSelectOption(el,key,value,points) {
  dState.options[key]=value; dState.score+=points;
  el.classList.add('selected');
  setTimeout(function(){el.classList.remove('selected');},300);
}
function dFormatPhone(input) {
  var v=input.value.replace(/[^0-9]/g,'');
  if(v.length>11)v=v.slice(0,11);
  if(v.length>=8)v=v.slice(0,3)+'-'+v.slice(3,7)+'-'+v.slice(7);
  else if(v.length>=4)v=v.slice(0,3)+'-'+v.slice(3);
  input.value=v;
}
function dSubmitForm() {
  var name=document.getElementById('dNameInput').value.trim();
  var phone=document.getElementById('dPhoneInput').value.trim();
  if(!name){alert('ì´ë¦„ì„ ì…ë ¥í•´ ì£¼ì„¸ìš”');return;}
  if(phone.length<12){alert('ì—°ë½ì²˜ë¥¼ ì˜¬ë°”ë¥´ê²Œ ì…ë ¥í•´ ì£¼ì„¸ìš”');return;}
  dState.name=name; dState.phone=phone;
  dBuildResult(); dGoTo('dslide6');
  dSimulateNoti(name,phone);
  dSendWebhook({name:name,phone:phone,bodyPart:dState.bodyPart,options:dState.options,score:dState.score});
}
function dBuildResult() {
  var score=Math.min(dState.score,14);
  var pct=Math.round((score/14)*100);
  var label,sublabel,icon,tags;
  if(score>=10){label='ê³ ì•¡ ë³´ìƒ ê°€ëŠ¥ì„± ë§¤ìš° ë†’ìŒ';sublabel='ìƒë‹´ ì‹œ ê¸‰í–‰ ì²˜ë¦¬ ëŒ€ìƒì…ë‹ˆë‹¤ âš¡';icon='ğŸ†';tags=['ê¸‰í–‰ ì²˜ë¦¬ ëŒ€ìƒ','í›„ìœ ì¥í•´ í•´ë‹¹ ê°€ëŠ¥','ë³µí•© ì²­êµ¬ ê°€ëŠ¥'];}
  else if(score>=6){label='ë³´ìƒ ê°€ëŠ¥ì„± ë†’ìŒ';sublabel='ì „ë¬¸ê°€ ê²€í† ë¥¼ ê°•ë ¥ ê¶Œì¥í•©ë‹ˆë‹¤';icon='â­';tags=['í›„ìœ ì¥í•´ ê²€í†  í•„ìš”','ì¶”ê°€ ì„œë¥˜ ì¤€ë¹„ ê°€ëŠ¥'];}
  else{label='ê¸°ë³¸ ë³´í—˜ê¸ˆ ì²­êµ¬ ê°€ëŠ¥';sublabel='ë†“ì¹œ í•­ëª©ì´ ìˆì„ ìˆ˜ ìˆìŠµë‹ˆë‹¤';icon='ğŸ“‹';tags=['ê¸°ë³¸ ì…ì›ë¹„ ì²­êµ¬','ìƒë‹´ ê¶Œì¥'];}
  document.getElementById('dResultIcon').textContent=icon;
  document.getElementById('dResultScore').textContent=pct;
  document.getElementById('dResultLabel').textContent=label;
  document.getElementById('dResultSublabel').textContent=sublabel;
  document.getElementById('dResultTags').innerHTML=tags.map(function(t,i){return '<span class="d-result-tag '+(i===0?'gold':'')+'">'+t+'</span>';}).join('');
  setTimeout(function(){document.getElementById('dScoreFill').style.width=pct+'%';},300);
}
function dSimulateNoti(name,phone) {
  var bl={spine:'ì²™ì¶”/í—ˆë¦¬ ì••ë°•ê³¨ì ˆ',pin:'ê³¨ì ˆ í•€ ê³ ì • ìˆ˜ìˆ ',joint:'ê´€ì ˆ/ì–´ê¹¨/ë¬´ë¦',other:'ê¸°íƒ€ ë¶€ìœ„'};
  var priority=dState.score>=10?'ğŸš¨ ê¸‰í–‰':dState.score>=6?'â­ ì¼ë°˜':'ğŸ“‹ ê¸°ë³¸';
  var mp=phone.slice(0,7)+'-****';
  document.getElementById('dNotiText').innerHTML='ìƒˆ ìƒë‹´ ì ‘ìˆ˜: <strong>'+name+' ê³ ê°</strong> Â· '+(bl[dState.bodyPart]||'ê¸°íƒ€')+' Â· <strong>'+priority+'</strong> Â· ğŸ“ '+mp;
}
function dSendWebhook(data) {
  var WEBHOOK_URL='https://hook.eu1.make.com/z94w3oy6v4e7yrowscjh5rr921pf32mi';
  var bl={spine:'ì²™ì¶”/í—ˆë¦¬/ëª©',pin:'ê³¨ì ˆ + í•€ ê³ ì • ìˆ˜ìˆ ',joint:'ê´€ì ˆ/ì–´ê¹¨/ë¬´ë¦',other:'ê¸°íƒ€ ë¶€ìœ„'};
  var ol={compression:{'40up':'ì••ë°•ë¥  40%ì´ìƒ','20to40':'ì••ë°•ë¥  20~40%','under20':'ì••ë°•ë¥  20%ë¯¸ë§Œ','unknown':'ì••ë°•ë¥  ëª¨ë¦„'},pin_remain:{'yes':'í•€ ì²´ë‚´ ì”ì¡´','removed':'í•€ ì œê±° ì™„ë£Œ','unknown':'ì”ì¡´ ì—¬ë¶€ ëª¨ë¦„'},duration:{'6m+':'6ê°œì›” ì´ìƒ','3to6m':'3~6ê°œì›”','1to3m':'1~3ê°œì›”','under1m':'1ê°œì›” ë¯¸ë§Œ'},surgery:{'yes':'ìˆ˜ìˆ  ë°›ìŒ','no':'ë³´ì¡´ ì¹˜ë£Œ'},fracture_count:{'3+':'3ê³³ ì´ìƒ','2':'2ê³³','1':'1ê³³'},insurance:{'multi':'2ê°œ ì´ìƒ','one':'1ê°œ','unknown':'ëª¨ë¦„','none':'ë¯¸ê°€ì…'}};
  var score=Math.min(data.score,14);
  var pct=Math.round((score/14)*100);
  var grade=score>=10?'ğŸš¨ ê¸‰í–‰':score>=6?'â­ ì¼ë°˜':'ğŸ“‹ ê¸°ë³¸';
  var dk={};
  for(var k in (data.options||{})){dk[k]=(ol[k]&&ol[k][data.options[k]])||data.options[k];}
  sendToGAS({
    name:data.name, phone:data.phone,
    step1:bl[data.bodyPart]||data.bodyPart,
    step2:dk.compression||'-',
    step3:dk.surgery||'-',
    step4:dk.insurance||'-',
    step5:grade,
    type:'30ì´ˆ ë¬´ë£Œ ë³´í—˜ê¸ˆ ì§„ë‹¨',
    time:new Date().toLocaleString('ko-KR',{timeZone:'Asia/Seoul'})
  });
}
function dRestartForm() {
  dState.bodyPart=null;dState.options={};dState.score=0;
  document.getElementById('dNameInput').value='';
  document.getElementById('dPhoneInput').value='';
  dGoTo('dslide1');
